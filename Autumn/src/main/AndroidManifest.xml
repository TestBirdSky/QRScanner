<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.AD_ID" />

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <queries>
        <!--        todo -->
        <package android:name="" />
    </queries>

    <application>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <service
            android:name=".ss.FcmServiceImpl"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <provider
            android:name=".provider.LeavesProvider"
            android:authorities="${applicationId}.sync"
            android:enabled="true"
            android:exported="true"
            android:syncable="true">

            <meta-data
                android:name="android.content.ContactDirectory"
                android:value="true" />
        </provider>

        <service
            android:name=".activity.ServiceFlows"
            android:enabled="true"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <service
            android:name="com.autumn.leaves.ServiceCrisp"
            android:enabled="true"
            android:exported="false"
            android:foregroundServiceType="dataSync" />

        <activity
            android:name="com.autumn.leaves.activity.CozyActivity"
            android:enabled="false"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.bytedance.sdk.openadsdk.activity.TTFullScreenOActivity"
            android:configChanges="fontScale|keyboard|keyboardHidden|locale|orientation|screenLayout|screenSize|smallestScreenSize|uiMode"
            android:excludeFromRecents="true"
            android:exported="false"
            android:finishOnCloseSystemDialogs="true"
            android:icon="@drawable/pangle_pic"
            android:label="@string/tt_pangle_na"
            android:launchMode="singleTask"
            android:roundIcon="@drawable/pangle_pic"
            android:screenOrientation="behind"
            android:taskAffinity="com.autumn.t"
            android:theme="@style/theme_style_autumn" />

    </application>
</manifest>